<div class="am-panel am-panel-default">
	<div class="am-panel-hd">组织机构详情</div>
	<div class="am-panel-bd clearfix">
		<div class="showRoleInfocss" style="color:#000000;">
			<form class="am-form am-form-horizontal" id="personalModify" method="post" onsubmit="return false">
				<div class="add-form clearfix">
					<div class="am-form-group am-u-sm-12">
						<label for="doc-ipt-1" class="am-u-sm-3 am-form-label">手机号：</label>
						<div class="am-u-sm-4">
							<input type="text" id="doc-ipt-1" placeholder="手机号"  name="mobilePhone" class="am-input-sm am-form-field " value="<%=this.mobilePhone%>">
						</div>
					</div>
					<div class="am-form-group am-u-sm-12">
						<label for="password" class="am-u-sm-3 am-form-label">电话号：</label>
						<div class="am-u-sm-4">
							<input type="input" id="password" name="officePhone" placeholder="电话号" class="am-input-sm am-form-field " value="<%=this.officePhone%>">
						</div>
					</div>
					<div class="am-form-group am-u-sm-12">
						<label for="password" class="am-u-sm-3 am-form-label">邮箱：</label>
						<div class="am-u-sm-4">
							<input type="input" id="password" placeholder="邮箱" name="email" class="am-input-sm am-form-field" value="<%=this.email%>">
						</div>
					</div>
				</div>
			</form>
		</div>
	</div>
</div>