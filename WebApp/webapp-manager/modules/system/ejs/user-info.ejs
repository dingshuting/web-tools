<div class="wrapper wrapper-content animated fadeInRight">
	<div class="row">
		<div class="col-lg-12">
			<div class="ibox float-e-margins">
				<div class="ibox-content">
					<div class="row">

						<form class="form-horizontal" method="post" onsubmit="return false" id="userFrom">
							<input readonly="readonly" type="hidden" name="roleStr" id="roleStr" />

							<div class="form-group  col-md-6">
								<label class="col-lg-4 control-label">登录账户</label>
								<div class="col-lg-7">
									<input readonly="readonly" value="<%=this.accountNo%>" type="text"  class="form-control" name="accountNo">
								</div>
							</div>

							<div class="form-group  col-md-6">
								<label for="doc-ipt-1" class="col-lg-4 control-label">用户姓名</label>
								<div class="col-lg-7">
									<input readonly="readonly" value="<%=this.name%>" type="text" id="doc-ipt-1" name="name" class="form-control">
								</div>
							</div>

							<!--<div class="form-group  col-md-6">-->
								<!--<label for="doc-ipt-1" class="col-lg-4 control-label">电子邮箱</label>-->
								<!--<div class="col-lg-7">-->
									<!--<input readonly="readonly" value="<%=this.email%>" type="text" name="email" class="form-control">-->
								<!--</div>-->
							<!--</div>-->

							<div class="form-group  col-md-6">
								<label for="doc-ipt-1" class="col-lg-4 control-label">手机号码</label>
								<div class="col-lg-7">
									<input readonly="readonly" value="<%=this.mobilePhone%>" type="text" name="mobilePhone" class="form-control">
								</div>
							</div>
							<!--<div class="form-group  col-md-6">-->
								<!--<label for="doc-ipt-1" class="col-lg-4 control-label">办公电话</label>-->
								<!--<div class="col-lg-7">-->
									<!--<input readonly="readonly" value="<%=this.officePhone%>" type="text" name="officePhone" class="form-control">-->
								<!--</div>-->
							<!--</div>-->
							<div class="form-group  col-md-6">
								<label for="doc-ipt-1" class="col-lg-4 control-label">添加时间</label>
								<div class="col-lg-7">
									<input readonly="readonly" value="<%=this.regTime%>" type="text" name="officePhone" class="form-control">
								</div>
							</div>
							<div class="form-group  col-md-6">
								<label for="doc-ipt-1" class="col-lg-4 control-label">岗位</label>
								<div class="col-lg-7">
									<input readonly="readonly" value="<%=this.title%>" type="text" name="title" class="form-control">
								</div>
							</div>
							<div class="form-group  col-md-6">
								<label for="doc-ipt-1" class="col-lg-4 control-label">岗位类型</label>
								<div class="col-lg-7">
									<% if (this.titleType==0) { %>
									<input readonly="readonly" value="全职" type="text" name="titleType" class="form-control">
									<% } else { %>
									<input readonly="readonly" value="兼职" type="text" name="titleType" class="form-control">
									<% } %>
									<!--<input readonly="readonly" value="<%=this.titleType==0?'兼职':'全职'%>" type="text" name="titleType" class="form-control">-->
								</div>
							</div>

						<div class="form-group  col-md-6">
							<label for="doc-ipt-1" class="col-lg-4 control-label">角色信息</label>
							<div class="col-lg-7" id="show_Roles">
								<%this.roles.each(function(current) { %>
								&nbsp;  <%=current.attr('name')%>  &nbsp;
								<%})%>
							</div>
						</div>

					<div class="form-group col-md-6">
						<div class="col-sm-4 col-sm-offset-2">
							<!--<button class="btn btn-white" type="button" name="cancel">取消</button>-->
						</div>
					</div>
					<!-- content end -->
					</form>
			</div>
			</div>
		</div>
	</div>
</div>
</div>