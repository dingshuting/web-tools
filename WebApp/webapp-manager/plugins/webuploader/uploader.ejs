<div id="uploader">
	<!--用来存放文件信息-->
	<div class="row">
		<div class="col-lg-12"> 
			<div class="uploader-list">
				<%this.each(function(current,index) { %>
				<div class="upload-list-item">
					<i class='fa fa-remove remove'></i>
					<img src="<%=current %>" />
				</div>
				<%})%>
				<div class="picker"><i class="fa fa-fw fa-plus"></i></div>
			</div>
			
		</div>
	</div>
</div>